{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "dialog:default",
    "dialog:allow-message",
    "dialog:allow-open",
    "deep-link:default",
    "deep-link:allow-get-current",
    "deep-link:allow-register",
    "deep-link:allow-is-registered",
    "log:default",
    "log:allow-log",
    "process:default"
  ]
}
